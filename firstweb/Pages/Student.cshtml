@page
@model NetOnWebPages.StudentModel 
@{
}

<h1>Students</h1>

<table class="table mt-5">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Class ID</th>
    </tr>

        @foreach (var Student in @Model.Students)
        {
        <tr>
            <td>@Student.Id</td>
            <td>@Student.Name</td>
            <td>@Student.ClassId</td>
        </tr>
        }
</table>